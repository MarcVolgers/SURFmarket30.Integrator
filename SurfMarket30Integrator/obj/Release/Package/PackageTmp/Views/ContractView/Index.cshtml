@model IEnumerable<SURF.Surfmarket30.Shared.Models.Contract>

@{
    ViewBag.Title = "View";
}

<h2>Contracten</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Revisie​)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DatumStart​)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DatumEind​)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DatumGetekend)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContractWaarde)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DocumentUrl)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.Name, "Details", new { id = item.Id })
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Revisie​)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DatumStart​)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DatumEind​)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DatumGetekend)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContractWaarde)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DocumentUrl)
        </td>
    </tr>
}

</table>
